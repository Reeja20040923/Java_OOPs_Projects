package polymorphismdemo.animal;

public interface Animal1 {
	void sound();

}
class Dog2 implements Animal1{
	@override
	public void sound() {
		System.out.println("Dog Barks");
	}
}

class Cat2 implements Animal1{
	@override
	public void sound() {
		System.out.println("Cat Meows");
	}
}

//Main Method
public class InterfacePolymorphim{
	public static void makingAnimalSound(Animal1 animal) {
		animal.sound();
	}
	
	public static void main(String[] args) {
		Animal dog = new Dog2();
		Animal cat = new Cat2();
		
		makingAnimalSound(dog);
		makingAnimalSound(cat);
	}
}
